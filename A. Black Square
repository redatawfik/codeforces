#include <bits/stdc++.h>

using namespace std;

int main()
{

    int arr[5];

    for(int i = 1; i < 5; i++){
        int v;
        cin >> v;
        arr[i] = v;
    }

    string a;
    cin >> a;
    int sum = 0;

    for(int i = 0, n = a.size(); i < n; i++){
         sum += arr[a[i] - '0'];
    }

    /*
    while(cal > 0){
        int num = cal % 10;
        sum += arr[num];

        cal /= 10;
    }
    */

    cout << sum;


}
