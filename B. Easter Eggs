import java.util.Scanner;

public class Solution {


    public static void main(String[] args) {


        Scanner scan = new Scanner(System.in);

        int n = scan.nextInt();

        String[] arr = {"G", "B", "I", "V"};

        int index = 0;
        System.out.print("ROY");
        for (int i = 0; i < n - 3; i++) {
            index = i % 4;

            System.out.print(arr[index]);

        }

    }

}
